import java.util.Scanner;

public class Assignment1 {

    public static void main(String[] args) {

        int celcius;
        double farenheit;

        for ( celcius = 0; celcius<=100; celcius=celcius+2){

            farenheit = (double) (1.8 * celcius) + 32;

            System.out.println("Celcius:" + celcius + " Farenheit: " + (int) farenheit);
        
        }      
    }
}
